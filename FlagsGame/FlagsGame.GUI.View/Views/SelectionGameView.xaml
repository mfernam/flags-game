<Window x:Class="FlagsGame.GUI.View.Views.SelectionGameView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlagsGame.GUI.View.Views"
        mc:Ignorable="d"
        Title="SelectionGameView" Height="203" Width="273" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="{DynamicResource competititon}" HorizontalAlignment="Left" Margin="30,102,0,0" VerticalAlignment="Top" Width="90" Height="50" Name="competition" Click="GameSelection_Click" Background="#FF129AB8"/>
        <Button Content="{DynamicResource trainning}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="156,102,0,0" Width="90" Height="50" Name="trainning" Click="GameSelection_Click" Background="#FFCDC407"/>
        <Label Content="{DynamicResource select_game}" HorizontalAlignment="Left" Margin="30,26,0,0" VerticalAlignment="Top" FontSize="16"/>

    </Grid>
</Window>
